         <value>none</value>
      </choice>
   </define>
   <define name="handbell-valueValues">
      <a:documentation>The handbell-value type represents the type of handbell technique being notated.</a:documentation>
      <choice>
         <value>damp</value>
         <value>echo</value>
         <value>gyro</value>
         <value>hand martellato</value>
         <value>mallet lift</value>
         <value>mallet table</value>
         <value>martellato</value>
         <value>martellato lift</value>
